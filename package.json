    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "acorn": "^6.1.1",
    "acorn-dynamic-import": "^4.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^2.0.1",
    "compression": "^1.7.4",
    "css-loader": "^2.1.1",
    "eslint": "^5.12.1",
    "eslint-plugin-import": "^2.15.0",
