    "material-icons": "^0.3.1",
    "next-core-logger": "^2.6.1",
    "presentation-application": "^2.8.3",
    "presentation-components": "^2.8.9",
    "presentation-css": "^3.3.0",
    "presentation-dialogs": "^2.7.1",
    "presentation-logger": "^2.6.4",
    "presentation-mediator": "^2.7.1",
    "presentation-router": "^2.6.7",
