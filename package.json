{
  "name": "create-augmented-app",
  "version": "0.1.6",
  "description": "webpack, babel, augmented, application, router, mediator, and sass.  Also includes hosting express service.",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --mode development --progress",
    "build": "webpack --mode production --progress",
    "lint": "eslint ./dist/clean-application-start.js",
    "test": "mocha --require @babel/register --require test/helper.js -c test/*Spec.js",
    "start": "node ./service/index.js",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/create-augmented-app.git"
  },
  "keywords": [
    "augmentedjs",
    "presentation"
  ],
  "author": "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
  "license": "Apache-2.0",
  "bin": {
    "create-augmented-app": "./bin/caa.js"
  },
  "preferGlobal": true,
  "devDependencies": {
